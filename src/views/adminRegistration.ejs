<%- contentFor('header') %>
<h1>Jenn's Ent</h1>

<%- contentFor('main') %>
<div class="title">
    <h3>Create a new admin account</h3>
    <p>It's quick and easy</p>
</div>
<form action="" method="post">
    <div class="name first">
        <input type="text" id="first-name" placeholder="First name" name="first-name">
        <input type="text" id="last-name" placeholder="Last name" name="last-name">
    </div>


    <div class="credentials"><input type="text" id="email" placeholder="Email" name="email"></div>
    
    <div class="credentials"><input type="password" id="password" placeholder="New password" name="password"></div>


    <div><input type="submit" id="submit" name="submit" value="Register!"></div>
</form>